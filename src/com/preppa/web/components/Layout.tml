                                    <div t:id="rightblockoneregularContent" align="center">
                                        <div class="droppable">
                                            <div t:type="ck/RoundCornerContainer" fgcolor="#e8eefa" bgcolor="#000000"
                                                 style="padding: 1px; margins: 1px;" align="center">
                                                <div style="padding: 1px; background-color:#e8eefa; margins: 1px;">
                                                    <div class="rightsideboxes">

                                                        Movable Block 1
                                                        <br  />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <br  />
                                    </div>
                                </t:block>
                                <t:delegate to="rightblocktwo" />
