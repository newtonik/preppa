<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
    <head>
        <title> ${pageTitle}</title>
        <link rel="shortcut icon" href="${asset:context:/images/favicon.ico}"/>
        <link rel="stylesheet" href="${styles}" type="text/css"/>
    </head>

    <body>
        <div id="fixed">
            <div id="header" class="container">
                <t:navbar/>

            </div>
            <div class="colmask holygrail">

                <div class="colmid">
                    <div class="colleft">


                        <div class="col1wrap">

                            <div class="col1">
                                <div id="mainwrap">

                                    <div id="contentwrap">
                                        <div id="content">
                                            <t:body/>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="col2">
                            <br/>
                            <div id ="newuser" t:type="ck/RoundCornerContainer" fgcolor="#e8eefa" bgcolor="#000000"
                                 style="padding: 1px; margins: 1px;" align="center">
                                <div style="padding: 1px; background-color:#e8eefa; margins: 1px;">
                                    <t:delegate to="leftContent" />
                                    <t:block t:id="sideboxes">
                                        <h2>Left Side Bar </h2>
                                        <div t:id="leftRegularContent">
                                            <br/>
                                            <div t:id="contribute" style="width: 150px;">
                                                <div class="leftlinks">
                                                    <a href="#" t:page="contribution/question/general/new" t:type="pagelink" >
                                                        Question
                                                    </a>
                                                </div>
                                                <br />
                                                <div class="leftlinks">
                                                    <a href="#" t:page="contribution/article/create" t:type="pagelink" >
                                                        Article
                                                    </a>
                                                </div>
                                                <br />
                                                <div class="leftlinks">
                                                    <a href="#" t:page="contribution/vocab/add" t:type="pagelink" >
                                                        Vocab
                                                    </a>
                                                </div>
                                            </div>
                                            <br/>
                                        </div>
                                    </t:block>
                                </div></div>
                            <div class="droppable">
                            </div>
                            <div class="droppable">
                            </div>
                        </div>

                    </div>
                </div>


                <div class="col3">
                    <br clear="none" />
                    <div align="center" style="height:100%">
                        <t:delegate to="rightblockone" />
                        <t:block>
                            <div t:id="rightblockoneregularContent" align="center">
                                <div class="droppable">
                                    <div t:type="ck/RoundCornerContainer" fgcolor="#e8eefa" bgcolor="#000000"
                                         style="padding: 1px; margins: 1px;" align="center">
                                        <div style="padding: 1px; background-color:#e8eefa; margins: 1px;">
                                            <div class="rightsideboxes">

                                                Movable Block 1
                                                <br  />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br  />
                            </div>
                        </t:block>
                        <t:delegate to="rightblocktwo" />
                        <t:block>
                            <h2></h2>
                            <div t:id="rightblocktworegularContent" >
                                <div class="droppable">
                                    <div t:type="ck/RoundCornerContainer" fgcolor="#e8eefa" bgcolor="#000000"
                                         style="padding: 1px; margins: 1px;" align="center">
                                        <div style="padding: 1px; background-color:#e8eefa; margins: 1px;">
                                            <div class="rightsideboxes">

                                                Movable Block 2
                                                <br  />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <br  />
                            </div>
                        </t:block>
                        <t:delegate to="rightblockthree" />
                        <t:block>
                            <h2></h2>
                            <div t:id="rightblockthreeregularContent" >
                                <div class="droppable">
                                    <div t:type="ck/RoundCornerContainer" fgcolor="#e8eefa" bgcolor="#000000"
                                         style="padding: 1px; margins: 1px;" align="center">
                                        <div style="padding: 1px; background-color:#e8eefa; margins: 1px;">
                                            <div class="rightsideboxes">
                                                Movable Block 3
                                                <br  />
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </t:block>
                        <br/>
                    </div>




                </div>


            </div>

            <div id="footer">
                (C) 2009 Preppa, Inc. Build ${message:build.number}
            </div>
        </div>
    </body>
</html>