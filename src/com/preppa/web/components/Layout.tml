 <!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
    <head>
        <title> ${pageTitle}</title>
        <link rel="shortcut icon" href="${asset:context:/images/favicon.ico}"/>
    </head>
    <body>
        <div id="header" class="container">
            <t:navbar/>
            Preppa
        </div>
    <div class="colmask holygrail">
         
        <div class="colmid">
         <div class="colleft">
           
            <div class=".roundedcornr_box">
            <div class="col1wrap">
                 <div class="roundedcornr_top"><div></div></div>
                <div class="col1">
                    <t:body/>
                </div>
                 <div class="roundedcornr_bottom"><div></div></div>
            </div>
                <div class="col2">
                     <t:delegate to="leftContent" />
                    <t:block t:id="sideboxes">
                    <h2>Left Side Bar </h2>
                     <div t:id="leftRegularContent">
                            Here come the left regular content
                     </div>
                     </t:block>
                </div>
                <div class="col3">
                
                      <div t:id="rightblockoneregularContent">
                        <h3>Block 1</h3>
                            Block 1 Contents
                    </div>
                 
                 <t:delegate to="rightblocktwo" />
                  <t:block>
                  <h2> Block 2 </h2>
                     <div t:id="rightblocktworegularContent" class="sideboxes">
                            Block 2 Contents
                    </div>
                  </t:block>
                 <t:delegate to="rightblockthree" />
                  <t:block>
                  <h2> Block 3 </h2>
                     <div t:id="rightblockthreeregularContent" class="sideboxes">
                            Block 3 Contents
                    </div>
                  </t:block>

                </div>
            </div>
         
            </div>
            </div>
              
        </div>
        
        <div id="footer">
            (C) 2009 Preppa, Inc. Build ${message:build.number}
        </div>

    </body>
</html>