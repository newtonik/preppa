<html t:type="layout" t:pageTitle="New Article" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">
        <br/>
        <br />
      <div id ="artcont" align="center">
          <t:form t:id="articleform">
             <table id="articleformtable">
                 <tr>
                     <td>
                         <label for="testsubject"> Section: </label>

                            <select t:type="ck/BeanSelect"
                            list="testsubjects"
                            labelField="literal:name"
                            valueField="literal:id"
                            value="testsubject"/>
                    </td>
                     
                     <td align="left">
                         <t:label for="topicks"> Topics: </t:label>
                         <input t:type="TextField" t:id="topicks" t:mixins="autocomplete" t:value="topicks"/>
                         </td>
                  </tr>
                  <tr  align="left">
                      <td >
                       <t:label for="fTitle"> Title: </t:label>
                      <input t:type="TextField" t:id="fTitle"  t:value="fTitle"/>
                      </td>

                 </tr>
                
                 <tr>
                     <td align="center">
                          <t:editor t:id="body" value="fBody" t:toolbarSet="Basic" t:width="400" t:height="300"/>

                     </td>
                 </tr>
                  <tr>
                      <td colspan="2" align="left">
                        <input type="submit" value="Submit"/>
                    </td>

                 </tr>
                 
             </table>
             </t:form>
    </div>
 

    


</html>