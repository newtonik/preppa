<html t:type="layout" t:pageTitle="New Vocab Word" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_0_0.xsd">

        <h3> Add a Vocab </h3>
        <p>
          <t:beaneditform t:id="vocab">

               <t:parameter name="partofspeech">
                <t:label for="partofspeech"/>
                <t:select t:id="partofspeech" value ="vocab.partofspeech" model="literal:Noun,Adjective, Pronoun, Adverb, Propositions, Conjuctions"/>
               </t:parameter>

               <t:parameter name="definition">
                <t:label for="definition"/>
                 <t:textarea t:id="definition" value="vocab.definition" size="10x80"/>
                </t:parameter>

                   <t:parameter name="examplesentence">
                <t:label for="examplesentence">Example</t:label>
                 <t:textarea t:id="examplesentence" value="vocab.examplesentence" size="10x80"/>
                </t:parameter>
          </t:beaneditform>
          
          <br/>
          <br/>
          <br/>
        </p>
</html>